<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="khk-twol">
    <pipeline>
      <program name="hfst-strings2fst -S"/>
      <program name="hfst-compose-intersect">
        <file name=".deps/khk.twol.hfst"/>
      </program>
      <program name="hfst-fst2strings"/>
    </pipeline>
  </mode>

  <mode name="khk-morph">
    <pipeline>
      <program name="hfst-proc -w">
        <file name="khk.automorf.hfst"/>
      </program>
    </pipeline>
  </mode>

  <mode name="khk-lexc">
    <pipeline>
      <program name="hfst-proc -g">
        <file name=".deps/khk.lexc.hfst"/>
      </program>
    </pipeline>
  </mode>


  <mode name="khk-gener">
    <pipeline>
      <program name="hfst-proc -g">
        <file name="khk.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>

  <mode name="khk-disam">
    <pipeline>
      <program name="hfst-proc -w">
        <file name="khk.automorf.hfst"/>
      </program>
      <program name="cg-proc -w">
        <file name="khk.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

</modes>
